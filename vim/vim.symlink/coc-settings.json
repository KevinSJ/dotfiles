{
  "npm.binPath": "yarn",
  "hover.target": "float",
  /* "coc.preferences.hoverTarget": "echo", */
  "coc.preferences.formatOnInsertLeave": true,
  /* "coc.preferences.enableFloatHighlight": false, */
  "coc.preferences.jumpCommand": "vsplit",
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.extensionUpdateCheck": "weekly",
  /* "coc.preferences.currentFunctionSymbolAutoUpdate": true, */
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    "node_modules"
  ],
  "suggest.detailMaxLength": 100,
  "suggest.noselect": false,
  "suggest.enablePreselect": true,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 5000,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.detailField": "menu",
  "suggest.snippetIndicator": "*",
  "suggest.triggerCompletionWait": 100,
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf93d",
    "file": "\uf723",
    "folder": "\uf115",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf6a6",
    "module": "\uf40d",
    "operator": "\uf915",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue71b"
  },
  "diagnostic.highlightOffset": 3,
  "diagnostic.errorSign": "×",
  "diagnostic.warningSign": "»",
  "diagnostic.infoSign": "!",
  "diagnostic.hintSign": "?",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.showUnused": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": "           => ",
  "diagnostic.virtualText": false,
  /* "diagnostic.messageTarget": "echo", */
  "diagnostic.messageTarget": "float",
  "codeLens.enable": true,
  "list.previewHighlightGroup": "Statement",
  "list.nextKeymap": "<C-e>",
  "list.previousKeymap": "<C-u>",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "css",
    "javascript",
    "javascriptreact",
    "typescriptreact",
    "typescript",
    "vue",
    "scss"
  ],
  "prettier.printWidth": 80,
  "prettier.eslintIntegration": false,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "signature.target": "float",
  /* "signature.target": "float", */
  "signature.enable": true,
  "yank.priority": 30,
  "explorer.width": 38,
  "explorer.quitOnOpen": true,
  "explorer.activeMode": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.file.tabCD": true,
  "explorer.autoExpandSingleNode": true,
  "explorer.openAction.changeDirectory": true,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.column.git.showIgnored": true,
  "explorer.keyMappingMode": "none",
  "explorer.keyMappings": {
    "u": "nodePrev",
    "e": "nodeNext",

    "*": "toggleSelection",
    "<tab>": "actionMenu",

    "n": "expanded?",
    "E": ["toggleSelection", "normal:j"],
    "U": ["toggleSelection", "normal:k"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "i": "expand",
    "s": "open:vsplit",
    "<cr>": ["expandable?", "expandOrCollapse", "open"],
    "o": ["expandable?", "expandOrCollapse", "open"],
    "t": "open:tab",
    "<bs>": "gotoParent",
    "l": "gotoParent",

    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "pp": "pasteFile",
    "dD": "delete",
    "Dd": "deleteForever",

    "a": "addFile",
    "M": "addDirectory",
    "cw": "rename",

    ".": "toggleHidden",
    "zh": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "B": "gotoSource:file",
    "b": "gotoSource:buffer",

    "[[": "sourcePrev",
    "]]": "sourceNext",

    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",

    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "git.enableGutters": true,
  "git.branchCharacter": "\uf408",
  "git.addGBlameToBufferVar": false,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": "                 ~ ",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "tslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": false,
  "html.format.enable": true,
  "suggest.languageSourcePriority": 99,
  "javascript.referencesCodeLens.enable": false,
  "javascript.showUnused": true,
  "javascript.suggest.names": true,
  "javascript.suggestionActions.enabled": true,
  "javascript.format.enabled": true,
  "javascript.suggest.includeCompletionsForImportStatements": true,
  "tsserver.formatOnType": false,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.enableJavascript": true,
  /* "tsserver.locale": "/Applications/Visual Studio Code.app/Contents/Resources/app/extensions/typescript-language-features", */
  "snippets.priority": 3,
  "snippets.autoTrigger": false,
  "snippets.userSnippetsDirectory": "~/.config/nvim/Ultisnips",
  "json.format.enable": true,
  "languageserver": {
    "lua":{
      "command": "/home/kevin/lua-language-server/bin/Linux/lua-language-server",
      "args": ["-E", "/home/kevin/lua-language-server/bin/Linux/main.lua"],
      "filetypes": ["lua"],
      "rootPatterns": [".git/"],
      "settings": {
        "Lua": {
          "workspace": {
            "library": {
              "/usr/share/nvim/runtime/lua": true,
              "/usr/share/nvim/runtime/lua/vim/lsp": true
            },
            "maxPreload": 2000,
            "preloadFileSize": 1000
          },
          "runtime": {
            "version": "Lua 5.4.3"
          },
          "diagnostics": {
            "enable": true,
            "globals": [
              "hs",
              "vim",
              "it",
              "describe",
              "before_each",
              "after_each"
            ],
            "disable": ["lowercase-global"]
          },
          "completion": {
            "keywordSnippet": "Disable"
          }
        }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": []
    }
  },
  "snippets.textmateSnippetsRoots": [],
  "snippets.extends": [{ "vue": "javascript" }],
  "html.filetypes": ["html", "handlebars", "htmldjango", "vue"],
  "vetur.useWorkspaceDependencies": true,
  "vetur.format.enable": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
  "vetur.format.defaultFormatter.scss": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.completion.useScaffoldSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescriptreact": "typescriptreact"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.optimizeStylesheetParsing": true,
  "cSpell.maxNumberOfProblems": 10,
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll",
    "init.vim"
  ],
  "yaml.format.enable": true,
  "coc-actions.hideCursor": true,
  "coc-actions.useCursorLine": true,
  "sourcekit.commandPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
  "cSpell.userWords": ["Innei", "Typegoose", "consola"],
  "smartf.timeout": 5000,
  "java.format.settings.url": "/home/ksjiang/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd"
}
