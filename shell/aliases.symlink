# git
alias g='git'
alias gs='git status -sb'
# copy the current branch name
alias gcb='git rev-parse --abbrev-ref HEAD | pbcopy'
# because i make this typo a lot
alias ggs='gs'

# github PR master
alias prstaging='hub pull-request -b staging --push -c'
#alias prmaster=hub pull-request -b $(git symbolic-ref refs/remotes/origin/HEAD | sed 's@^refs/remotes/origin/@@') --push -c
alias prmaster='glab mr create -b master --wip -d "$(cat ~/.gitlab_mr_template)"'
alias exclusiveTests='sed -i "s/describe/describe.only/"  $(find . -type f -name "*.spec.js")'
alias disExclusiveTests='sed -i "s/\.only//g"  $(find . -type f -name "*.spec.js")'
